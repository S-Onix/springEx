<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="memberRegSvc" class="service.MemberRegisterService">
	</bean>

	<bean id="changePwdSvc" class="service.ChangePasswordService">
	</bean>

	<bean id="listPrinter" class="view.MemberListPrinter">
	</bean>
	
	<bean id="infoPrinter" class="view.MemberInfoPrinter">
	</bean>

  	<bean id="versionPrinter" class="view.VersionPrinter">
 		<property name="majorVersion" value="4" />
		<property name="minorVersion" value="1" />
 	</bean>
 
 </beans>